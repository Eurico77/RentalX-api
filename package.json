    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "seed:admin": "ts-node -r tsconfig-paths/register src/shared/infra/typeorm/seeds/admin.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --ext .ts --fix",
    "test": "jest --passWithNoTests --detectOpenHandles --no-cache --runInBand",
    "test:watch": "yarn test --watch --passWithNoTests --detectOpenHandles --no-cache --runInBand"
  },
  "engines": {
    "node": ">=14"
